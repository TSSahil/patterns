	for(m=0;m<10;m++)
	{
		for(i=0;i<5;i++)
		{
			for(j=0;j<i;j++)
			{
				printf(" ");
			}
			for(k=0;k<10-(2*i);k++)
			{
				printf("*");
				for(n=0;n<10000000;n++);
			}
			printf("\n");
		}
		printf("     *");
		for(i=0;i<5;i++)
		{
			for(j=5;j>i;j--)
			{
				printf(" ");
			}
			for(k=0;k<i*2;k++)
			{
				printf("*");
			}
			printf("\n");
		}
	}
		
